{
  "name": "goit-final-project-backend-group-3",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node --experimental-strip-types -r dotenv/config ./bin/www",
    "dev": "nodemon -e js,ts --experimental-strip-types -r dotenv/config ./bin/www",
    "migrate:up": "npm x --node-arg='--experimental-strip-types' sequelize-cli db:migrate",
    "seed": "npm x --node-arg='--experimental-strip-types' sequelize-cli db:seed:all",
    "test": "cross-env NODE_OPTIONS=\"--experimental-strip-types --experimental-vm-modules\" DB_HOST=localhost DB_PORT=5432 DB_USERNAME=test DB_PASSWORD=test DB_DATABASE=test JWT_SECRET=test jest",
    "test:watch": "cross-env NODE_OPTIONS=\"--experimental-strip-types --experimental-vm-modules\" DB_HOST=localhost DB_PORT=5432 DB_USERNAME=test DB_PASSWORD=test DB_DATABASE=test JWT_SECRET=test jest --watch"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^17.2.3",
    "express": "^4.22.1",
    "express-rate-limit": "^8.2.1",
    "joi": "^18.0.2",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.1",
    "ms": "^2.1.3",
    "multer": "^2.0.2",
    "nanoid": "^5.1.6",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.1",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "cross-env": "^10.1.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6"
  }
}
